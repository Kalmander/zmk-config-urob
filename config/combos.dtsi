/ {
	macros {
		combo_spacec_macro: combo_spacec_macro {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&two_kps SPACE C>, <&two_kps SPACE LS(C)>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};
		
		combo_au_macro: combo_au_macro {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&two_kps A U>, <&two_kps LS(A) U>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};
		
		combo_wh_macro: combo_wh_macro {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&two_kps W H>, <&two_kps LS(W) H>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};


		space_caps: space_caps {
			compatible = "zmk,behavior-macro";
			#binding-cells = <0>;
			wait-ms = <0>;
			tap-ms = <0>;
			bindings = <&kp SPACE &capsword>;
		};


		combo_ck_macro: combo_ck_macro {
			compatible = "zmk,behavior-mod-morph";
			#binding-cells = <0>;
			bindings = <&two_kps C K>, <&two_kps LS(C) K>;
			mods = <(MOD_LSFT|MOD_RSFT)>;
		};

    
